<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hibernateTest.dto">
    <class name = "pl.tp.server.MovesHistory" table = "name">
        <property name="source" column="source" />
        <property name="destination" column="destination" />
        <property name="price" column="PRICE" type="float" />

        <many-to-one name="parent" class="MovesHistory"
                     column="game_id" not-null="true"/>
    </class>
</hibernate-mapping>